{
  "name": "more-svelte-components",
  "version": "1.0.0",
  "type": "module",
  "description": "Svelte components for More MUD",
  "scripts": {
    "build": "vite build",
    "build:component": "node build-component.js",
    "build:hello-world": "node build-component.js hello-world",
    "build:counter": "node build-component.js counter",
    "build:3d-cube": "node build-component.js 3d-cube",
    "build:world-scene": "node build-component.js world-scene",
    "build:all": "npm run build:hello-world && npm run build:counter && npm run build:3d-cube && npm run build:world-scene",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@tsconfig/svelte": "^5.0.0",
    "@types/three": "^0.179.0",
    "svelte": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0"
  },
  "dependencies": {
    "@threlte/core": "^8.1.4",
    "@threlte/extras": "^8.1.4",
    "three": "^0.160.0"
  }
}